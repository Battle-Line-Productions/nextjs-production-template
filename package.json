{
  "name": "nextjs-production-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"*.{js,jsx,ts,tsx,json}\" --config ./.prettierrc.js",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "cypress": "pnpx cypress open",
    "cypress:test:chrome": "cypress run --browser chrome",
    "cypress:test:edge": "cypress run --browser edge",
    "cypress:test:firefox": "cypress run --browser firefox",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@next/env": "^13.4.7",
    "@reduxjs/toolkit": "^1.9.5",
    "bootstrap": "^5.3.0",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.1.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.24",
    "@storybook/addon-essentials": "7.0.24",
    "@storybook/addon-interactions": "7.0.24",
    "@storybook/addon-links": "7.0.24",
    "@storybook/addon-viewport": "^7.0.24",
    "@storybook/blocks": "7.0.24",
    "@storybook/jest": "^0.1.0",
    "@storybook/nextjs": "7.0.24",
    "@storybook/react": "7.0.24",
    "@storybook/testing-library": "0.0.14-next.2",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "css-loader": "^6.8.1",
    "cypress": "^12.16.0",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "sb": "^7.0.24",
    "storybook": "7.0.24",
    "style-loader": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm coverage"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "pnpm": {
    "shamefully-hoist": false,
    "public-hoist-pattern": [
      "**/react",
      "**/react-dom"
    ],
    "recursive-install": false,
    "ignore-pnpmfile": true
  }
}